t    \t</div>\n\t\t\t      \t    </div>\n                          \n\t\t\t\t\t\t<form @submit.prevent='login' action=\"#\" class=\"signin-form\">\n\t\t\t      \t\t\t<div class=\"form-group mb-3\">\n\t\t\t      \t\t\t\t<label class=\"label\" for=\"name\">Email</label>\n\t\t\t      \t\t\t\t<input type=\"text\" v-model=\"loginEmail\" class=\"form-control\" placeholder=\"email@mail.com\" required>\n\t\t\t      \t\t\t</div>\n\t\t            \t\t<div class=\"form-group mb-3\">\n\t\t            \t\t\t<label class=\"label\" for=\"password\">Password</label>\n\t\t            \t\t  <input type=\"password\" v-model=\"loginPswd\" class=\"form-control\" placeholder=\"Password\" required>\n\t\t            \t\t</div>\n\t\t            \t\t<div class=\"form-group\">\n\t\t            \t\t\t<button type=\"submit\" class=\"form-control btn btn-primary submit px-3\">Sign In</button>\n\t\t            \t\t</div>\n\t\t            \t\t<!-- <div class=\"form-group d-md-flex\"> -->\n\t\t            \t\t\t<!-- <div class=\"w-50 text-left\">\n\t\t\t        \t\t    \t<label class=\"checkbox-wrap checkbox-primary mb-0\">Remember Me\n\t\t\t\t\t\t\t\t\t\t\t  <input type=\"checkbox\" checked>\n\t\t\t\t\t\t\t\t\t\t\t  <span class=\"checkmark\"></span>\n\t\t\t\t\t\t\t\t\t</label>\n\t\t\t\t\t\t\t\t</div> -->\n\t\t\t\t\t\t\t\t<!-- <div class=\"w-50 text-md-right\">\n\t\t\t\t\t\t\t\t\t<a href=\"#\">Forgot Password</a>\n\t\t\t\t\t\t\t\t</div> -->\n\t\t            \t\t<!-- </div> -->\n\t\t          \t\t</form>\n\t\t        </div>\n\t\t      </div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</section>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Swal from 'sweetalert2'\nexport default {\n\tcomponents: {\n\t\t\n\t},\n\tdata() {\n\t\treturn {\n\t\t\tloginEmail : '',\n\t\t\tloginPswd : '',\n\t\t}\n\t},\n\tprops: ['baseUrl', 'changeIsLogin', 'changeDisplayLogin'],\n\tmethods : {\n\t\tlogin(){\n\t\t\taxios({\n\t\t\t\tmethod: 'POST',\n\t\t\t\turl: `${this.baseUrl}login`,\n\t\t\t\tdata: {\n\t\t\t\t\temail : this.loginEmail,\n\t\t\t\t\tpassword: this.loginPswd\n\t\t\t\t}\n\t\t\t})\n\t\t\t.then(({data})=>{\n\t\t\t\t// console.log(data)\n\t\t\t\tlocalStorage.setItem('access_token', data.access_token)\n\t\t\t\tthis.changeIsLogin(true)\n\t\t\t})\n\t\t\t.catch(err=>{\n\t\t\t\tSwal.fire(\n\t\t\t\t\terr.response.data.msg,\n\t\t\t\t\t'Try Again',\n\t\t\t\t\t'error'\n\t\t\t\t)\n\t\t\t})\n\t\t}\n\t}\n}\n</script>\n    \n<style>\n\t.container {\n\t\t/* border: 1px solid; */\n\t\theight: 100vh;\n\t}\n</style>"},"lineCount":null}},"error":null,"hash":"226c72d6b5a599c6e2f321b44a0fbb35","cacheData":{"env":{}}}